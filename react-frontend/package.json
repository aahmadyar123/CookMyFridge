{
  "name": "react-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/material": "^5.12.3",
    "@mui/styled-engine-sc": "^5.12.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^1.4.0",
    "material-ui": "^0.20.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^3.11.0",
    "react-native-ratings": "^8.1.0",
    "react-native-star-rating-widget": "^1.7.1",
    "react-router-dom": "^6.11.1",
    "react-scripts": "3.4.3",
    "react-star-rating-component": "^1.4.1",
    "react-stars": "^2.2.5",
    "styled-components": "^6.0.0-rc.1"
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "scripts": {
    "start":
      "export NODE_OPTIONS=--openssl-legacy-provider;HTTPS=false SSL_CRT_FILE=./.reactcert/cert.pem SSL_KEY_FILE=./.reactcert/key.pem react-scripts start",
    "build": "react-scripts build",
    "build-internal": "react-scripts --openssl-legacy-provider build",
    "test": "react-scripts --openssl-legacy-provider test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
